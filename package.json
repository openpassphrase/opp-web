{
  "name": "opp",
  "version": "1.1.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:with-server": "ng serve --proxy-config proxy.conf.json --configuration local-prod",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "build:prod": "node make_version.js && ng build --prod --base-href /opp --deploy-url opp/",
    "build:demo": "ng build --configuration demo --base-href /demo --deploy-url demo/",
    "build:test": "ng build --configuration local-prod",
    "build:stats": "ng build --configuration local-stats",
    "server:dist": "http-server ./dist/web/ -S -C cert/localhost.crt -K cert/localhost.key -P http://localhost:5000",
    "source-map-explorer": "source-map-explorer dist/web/*es2015.js",
    "bundle-report": "webpack-bundle-analyzer dist/web/stats-es2015.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.0",
    "@angular/cdk": "^11.2.0",
    "@angular/common": "^11.2.0",
    "@angular/compiler": "^11.2.0",
    "@angular/core": "^11.2.0",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^11.2.0",
    "@angular/material": "^11.2.0",
    "@angular/platform-browser": "^11.2.0",
    "@angular/platform-browser-dynamic": "^11.2.0",
    "@angular/router": "^11.2.0",
    "@angular/service-worker": "^11.2.0",
    "@auth0/angular-jwt": "^4.0.0",
    "@datorama/akita": "^4.22.1",
    "@ng-expandable-input/cdk": "^2.0.0",
    "@ng-expandable-input/material": "^2.0.0",
    "@ngspot/ngx-errors": "^1.0.1",
    "core-js": "^3.1.3",
    "ngx-clipboard": "13.0.1",
    "papaparse": "^5.2.0",
    "rxjs": "^6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.0",
    "@angular/cli": "^11.2.0",
    "@angular/compiler-cli": "^11.2.0",
    "@angular/language-service": "^11.2.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^13.11.1",
    "@types/papaparse": "^5.0.4",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-mocks": "^11.6.0",
    "ng-packagr": "^11.2.1",
    "pre-commit": "^1.2.2",
    "protractor": "~7.0.0",
    "source-map-explorer": "^2.4.2",
    "ts-node": "~8.8.2",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5",
    "webpack-bundle-analyzer": "^3.7.0"
  },
  "akitaCli": {
    "customFolderName": "true",
    "basePath": "./src/app/"
  },
  "pre-commit": [
    "lint",
    "build:test"
  ]
}
