{
  "name": "opp",
  "version": "1.1.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "start:prod": "ng serve --proxy-config proxy.conf.json --prod",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:prod": "node make_version.js && ng build --prod --base-href /opp --deploy-url opp/",
    "build:demo": "node make_version.js && ng build --configuration demo --base-href /demo --deploy-url demo/",
    "build:test": "node make_version.js && ng build --configuration local-prod",
    "server:dist": "http-server ./dist/web/ -S -C cert/localhost.crt -K cert/localhost.key -P http://localhost:5000",
    "precommit": "yarn lint && yarn build:prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/cdk": "^6.4.2",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/flex-layout": "^6.0.0-beta.17",
    "@angular/forms": "^6.1.0",
    "@angular/material": "^6.4.2",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/pwa": "^0.7.5",
    "@angular/router": "^6.1.0",
    "@angular/service-worker": "^6.1.0",
    "@auth0/angular-jwt": "^2.0.0",
    "@datorama/akita": "^1.6.2",
    "core-js": "^2.5.4",
    "immer": "^1.5.0",
    "ng2-validation": "^4.2.0",
    "ngx-clipboard": "^11.1.1",
    "ngx-messages": "^0.3.0",
    "rxjs": "^6.0.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular/cli": "^6.1.5",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "pre-commit": "^1.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.9.0"
  },
  "akitaCli": {
    "customFolderName": "true",
    "basePath": "./src/app/"
  },
  "pre-commit": [
    "lint",
    "build:test"
  ]
}
